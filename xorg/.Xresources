! Firecamp Theme aka Base16 ParaisoDark (~halfo)
*.foreground:   #a39e9b
*.background:   #190e18
*.cursorColor:  #4e4857

*.color0:       #2f1e2e
*.color1:       #fe5f57
*.color2:       #26c83f
*.color3:       #febd2d
*.color4:       #3498db
*.color5:       #4e4857
*.color6:       #1abc9c
*.color7:       #a39e9b

*.color8:       #776e71
*.color9:       #fe5f57
*.color10:      #26c83f
*.color11:      #febd2d
*.color12:      #3498db
*.color13:      #4e4857
*.color14:      #1abc9c
*.color15:      #e7e9db

! Terminal
URxvt.scrollBar: false
URxvt.vbell: false
URxvt.letterSpace: 0
URxvt.lineSpace: 7
URxvt.fading: 25
URxvt.font: xft:Meslo\ LG\ L\ DZ\ for\ Powerline:size=12:antialias=true,\
            -*-unifont-*-*-*-*-*-*-*-*-*-*-*-*

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1

URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/e800

*vbell: false

! Rofi things
rofi.lines: 10
rofi.hide-scrollbar: true
rofi.bw: 0
rofi.font: DejaVu Sans 16
rofi.separator-style: none
rofi.prompt: launch
rofi.p: launch

rofi.kb-remove-to-eol: Control+Delete
rofi.kb-accept-entry: Return,KP_Enter
rofi.kb-row-up: Control+k
rofi.kb-row-down: Control+j

! ------------------------------------------------------------------------------
! ROFI Color theme (bugando)
! ------------------------------------------------------------------------------
rofi.color-enabled: true
!rofi.color-window: argb:CC000000, argb:0000000, argb:0000000
!rofi.color-normal: argb:0000000, #fff4aa, #cd9c00, #586e75, #eee8d5
!rofi.color-active: argb:2b000000, #f82, argb:0eee8d5, argb:58fff166, #eee8d5
